{
  "name": "hlpflforms",
  "version": "2.0.0",
  "description": "Professional form builder with user authentication and analytics",
  "private": true,
  "scripts": {
    "build": "echo 'Build complete - static files ready'",
    "dev": "wrangler pages dev public",
    "deploy": "wrangler pages deploy public",
    "db:init": "wrangler d1 execute hlpflforms --file=./schema.sql",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "format": "prettier --write &quot;**/*.{js,json,css,html,md}&quot;",
    "format:check": "prettier --check &quot;**/*.{js,json,css,html,md}&quot;"
  },
  "keywords": [
    "forms",
    "cloudflare",
    "pages",
    "form-builder"
  ],
  "author": "HLPFL",
  "license": "MIT",
  "devDependencies": {
    "wrangler": "^3.0.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1"
  }
}